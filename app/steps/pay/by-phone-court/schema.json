{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "payPhoneCourt",
    "type": "object",
    "properties": {
        "paymentTimeToCall": {
            "type": "string",
            "enum": [
                "morning",
                "afternoon",
                "anytime"
            ]
        },
        "paymentPhoneNumber": {
            "type": "string",
            "pattern": "(\\+\\d\\d){0,1} {0,1}(\\(0\\)){0,1} {0,1}([\\d] {0,1}){9,12}"
        }
    },
    "required": [
        "paymentPhoneNumber",
        "paymentTimeToCall"
    ]
}
