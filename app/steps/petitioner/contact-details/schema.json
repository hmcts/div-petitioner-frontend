

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "petitioner-contact-details",
  "type": "object",
  "properties": {
    "petitionerEmail": {
      "type": "string",
      "format": "email"
    },
    "petitionerPhoneNumber": {
      "type": "string",
      "minLength": 9,
      "pattern": "^[0-9 +().-]+$"
    },
    "petitionerConsent": {
      "type": "string"
    }
  },
  "required": [
    "petitionerConsent"
  ]

}
