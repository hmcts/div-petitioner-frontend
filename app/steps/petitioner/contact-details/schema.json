

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "petitioner-contact-details",
  "type": "object",
  "properties": {
    "petitionerPhoneNumber": {
      "type": "string",
      "minLength": 9,
      "pattern": "^[0-9 +().-]+$"
    },
    "petitionerConsent": {
      "type": "string",
      "enum": ["Yes"]
    }
  },
  "required": [
    "petitionerConsent"
  ]

}
