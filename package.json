    "sinon-chai": "^2.8.0",
    "sinon-stub-promise": "^4.0.0",
    "sonar-scanner": "^1.0.1",
    "supertest": "^2.0.0",
    "w3c-validate": "0.0.2",
    "webdriverio": "^4.12.0"
  },
  "nyc": {
    "report-dir": "coverage",
    "temp-directory": "./coverage/.nyc_output",
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
