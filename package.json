{
  "name": "divorce-frontend",
  "description": "Apply for a divorce",
  "version": "3.0.0",
  "private": true,
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "start": "node server.js",
    "start-basic": "NODE_ENV=production NODE_PATH=. node server",
    "start-dev": "NODE_ENV=development NODE_PATH=. grunt start-dev",
    "start-debug": "NODE_ENV=development NODE_PATH=. node --inspect=0.0.0.0:9229 server.js",
    "start-debug-ide": "NODE_ENV=development NODE_PATH=. node $NODE_DEBUG_OPTION server.js",
    "start-prod": "NODE_PATH=. grunt start-prod",
    "setup-assets": "NODE_PATH=. grunt setup-assets",
    "setup": "NODE_PATH=. grunt setup-prod",
    "test": "NODE_PATH=. NODE_ENV=testing nyc mocha 'app/**/*.test.js' -t 10000",
    "test:coverage": "nyc check-coverage --statements 80 --branches 80 --functions 80 --lines 80",
    "test:a11y": "NODE_ENV=testing LOG_LEVEL=OFF NODE_PATH=. mocha test/accessibility/ --reporter spec --recursive --timeout 15000",
    "test:nsp": "nsp check --preprocessor yarn",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test:smoke": "NODE_ENV=testing NODE_PATH=. ./node_modules/.bin/codeceptjs run -c test/end-to-end/smoke.conf.js --reporter mocha-multi",
    "test:functional": "./bin/run-functional-tests.sh",
    "test-unit": "NODE_ENV=testing LOG_LEVEL=OFF NODE_PATH=. mocha './app/**/*.test.js' --reporter spec --recursive -t 3000",
    "test-unit-ide": "NODE_ENV=testing LOG_LEVEL=OFF NODE_PATH=. mocha $NODE_DEBUG_OPTION './app/**/*.test.js' --reporter spec --recursive",
    "test-validation": "NODE_ENV=testing LOG_LEVEL=OFF NODE_PATH=. mocha test/validation/ --reporter spec --recursive --timeout 15000",
    "test-e2e": "NODE_ENV=testing LOG_LEVEL=OFF NODE_PATH=. ./node_modules/.bin/codeceptjs run-multiple parallel -c test/end-to-end/codecept.conf.js --steps --reporter mocha-multi",
    "test-overnight-e2e": "NODE_ENV=testing LOG_LEVEL=OFF NODE_PATH=. codeceptjs run -c test/end-to-end/codecept.conf.js --steps",
    "test-crossbrowser-e2e": "NODE_ENV=testing LOG_LEVEL=OFF NODE_PATH=. codeceptjs run -c test/end-to-end/saucelabs.conf.js --steps",
    "test-all": "NODE_ENV=testing LOG_LEVEL=OFF yarn run test:coverage && yarn run test:a11y && yarn run test:nsp",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner",
    "lint": "NODE_PATH=. eslint ."
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@hmcts/div-idam-express-middleware": "https://github.com/hmcts/div-idam-express-middleware#6.0.0",
    "@hmcts/div-pay-client": "https://github.com/hmcts/div-pay-client#6.0.0",
    "@hmcts/div-service-auth-provider-client": "https://github.com/hmcts/div-service-auth-provider-client#3.0.0",
    "@hmcts/nodejs-healthcheck": "^1.2.0",
    "@hmcts/nodejs-logging": "^3.0.0",
    "add": "^2.0.6",
    "ajv": "^4.10.0",
    "applicationinsights": "^1.0.2",
    "body-parser": "^1.14.1",
    "co": "^4.6.0",
    "config": "^1.21.0",
    "connect-redis": "3.1.0",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.11",
    "csurf": "^1.9.0",
    "dotenv": "^5.0.0",
