    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "formidable": "^1.1.1",
    "glob": "^7.1.1",
    "govuk-elements-sass": "~3.0.2",
    "govuk_frontend_toolkit": "~6.0.1",
    "govuk_template_jinja": "https://github.com/alphagov/govuk_template/releases/download/v0.17.0/jinja_govuk_template-0.17.0.tgz",
    "govuk_template_mustache": "^0.19.0",
    "helmet": "^3.6.0",
