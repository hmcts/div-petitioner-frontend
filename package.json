    "flat": "^4.0.0",
    "grunt": "1.0.1",
    "grunt-cli": "1.2.0",
    "grunt-concurrent": "2.3.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-nodemon": "0.4.2",
    "grunt-sass": "^1.1.0",
    "grunt-sync": "^0.6.2",
